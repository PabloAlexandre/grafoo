{
  "name": "@grafoo/preact",
  "version": "0.0.1",
  "description": "the grafoo client preact bindings",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "malbernaz<albernazmiguel@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "preact": ">=8"
  },
  "scripts": {
    "prepare": "grafoo-bundle --input src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage && codecov"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js)$": "<rootDir>/../../scripts/jest-setup.js"
    }
  }
}
