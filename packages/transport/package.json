{
  "name": "@grafoo/transport",
  "description": "the grafoo client standard transport",
  "version": "0.0.1",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "typings": "dist/transport/src/index.d.ts",
  "source": "src/index.ts",
  "author": "malbernaz<albernazmiguel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "microbundle --compress false",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js)$": "<rootDir>/../../scripts/jest-setup.js"
    }
  }
}
