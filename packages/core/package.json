{
  "name": "@grafoo/core",
  "version": "0.0.1-alpha.6",
  "description": "the grafoo client core",
  "repository": "https://github.com/grafoojs/grafoo/tree/master/packages/core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "malbernaz<albernazmiguel@gmail.com>",
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "graphql",
    "graphql-client",
    "grafoo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "grafoo-bundle --input src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage && codecov"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js)$": "<rootDir>/../../scripts/jest-setup.js"
    }
  },
  "dependencies": {
    "@grafoo/transport": "^0.0.1-alpha.6",
    "@grafoo/types": "^0.0.1-alpha.6"
  },
  "devDependencies": {
    "grafoo-bundle": "^0.0.1-alpha.6"
  },
  "gitHead": "e041b3b71a4ff82fbdcaf0bb01db35243c4fdde8"
}
