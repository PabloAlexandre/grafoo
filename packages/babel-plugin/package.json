{
  "name": "@grafoo/babel-plugin",
  "version": "0.0.1-beta.15",
  "description": "grafoo client babel plugin",
  "repository": "https://github.com/grafoojs/grafoo/tree/master/packages/babel-plugin",
  "main": "dist/index.js",
  "author": "malbernaz<albernazmiguel@gmail.com>",
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "graphql",
    "graphql-client",
    "grafoo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js)$": "<rootDir>/../../scripts/jest-setup.js"
    },
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "babel-literal-to-ast": "^2.0.0",
    "graphql": "^0.13.1",
    "graphql-query-compress": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.50",
    "@babel/core": "^7.0.0-beta.50",
    "@babel/preset-env": "^7.0.0-beta.50",
    "babel-plugin-tester": "^5.0.0"
  },
  "gitHead": "3eee7fd65fe2b16191eb4c721746593986aeea06"
}
